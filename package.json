{
  "name": "fhevm-universal-sdk",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK - Framework-agnostic encryption SDK for confidential smart contracts",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "dev": "npm run dev --workspace=examples/nextjs-showcase",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fhevm",
    "zama",
    "encryption",
    "blockchain",
    "privacy",
    "fhe",
    "confidential-computing",
    "web3",
    "sdk"
  ],
  "author": "FHEVM SDK Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.0",
    "eslint": "^8.57.0",
    "prettier": "^3.6.2",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
